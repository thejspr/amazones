{
  "manifest_version": 2,
  "name": "Amazones",
  "version": "0.0.1",
  "description": "Redirects to amazon.co.uk",
  "content_scripts": [
    {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
  ],
  "icons": {
    "48": "icon2.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "background": "background.html"
}
